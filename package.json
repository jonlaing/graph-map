  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts",
    "start": "npm run build -- --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "map",
    "search",
    "index",
    "type",
    "property",
    "key"
  ],
  "author": "Jon Laing <jon@jonlaing.io>",
